{
    "name": "latamcreativa",
    "main": ".open-next/worker.js",
    "compatibility_date": "2024-09-23",
    "compatibility_flags": [
        "nodejs_compat"
    ],
    "assets": {
        "directory": ".open-next/assets",
        "binding": "ASSETS"
    },
    // R2 Bucket binding - conecta tu bucket R2 al Worker
    "r2_buckets": [
        {
            "binding": "R2_BUCKET",
            "bucket_name": "latamcreativa-storage"
        }
    ]
}